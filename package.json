{
  "name": "newming-web-next",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --mode development",
    "staging": "vite --mode staging",
    "prod": "vite --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "zip:prod": "bestzip dist.zip dist/",
    "preview": "vite preview",
    "test:dev": "vitest --mode development",
    "test:staging": "vitest --mode staging",
    "test:prod": "vitest --mode production",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@vueuse/core": "^8.3.1",
    "axios": "^0.27.1",
    "dayjs": "^1.11.1",
    "flowbite": "^1.6.3",
    "lodash": "^4.17.21",
    "pinia": "^2.0.13",
    "ua-parser-js": "^1.0.32",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-gtag-next": "^1.14.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.182",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "bestzip": "^2.2.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.19",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "sass": "^1.51.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.23.4",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  }
}
